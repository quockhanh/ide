import { Page, Browser, chromium } from "playwright";
import { PageDefinitions } from "../src/pageDefinitions";
import "../src/customMatcher";

let browser: Browser;
let page: Page;

beforeAll(async () => {
    browser = await chromium.launch({ headless: false });
    page = await browser.newPage();
});

afterAll(async () => {
    page.close();
    browser.close();
});

